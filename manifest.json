{
  "name": "StreetEasy Transit Time",
  "description": "Shows transit time on street easy.",
  "version": "0.1",
  "manifest_version": 2,
  /*
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  */
  "content_scripts": [{
    "matches": [
      "*://streeteasy.com/for-rent/*",
      "*://streeteasy.com/for-sale/*"
    ],
    "js": ["config.js", "neighborhood_map.js", "jquery.js", "main.js"],
    "run_at": "document_end"
  }],
  "permissions": ["activeTab", "https://maps.googleapis.com/*", "http://localhost/*"],
  "browser_action": {
      "default_title": "Print this page"
      //TODO
      //"default_icon": ""
  }
  // TODO
  //"icons": { "16": "", "128": "" }
}

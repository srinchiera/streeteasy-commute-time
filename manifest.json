{
  "name": "StreetEasy Transit Time",
  "description": "Shows transit time on street easy.",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "*://streeteasy.com/for-rent/*",
      "*://streeteasy.com/for-sale/*"
    ],
    "js": [ "jquery.js",
            "config.js",
            "utils.js",
            "neighborhood_map.js",
            "commute.js",
            "main.js" ],
    "run_at": "document_end"
  }],
  "permissions": ["storage", "activeTab", "https://maps.googleapis.com/*", "http://localhost/*"],
  "browser_action": {
      "default_title": "StreetEasy Transit Time",
      "default_popup": "popup.html"
      //TODO
      //"default_icon": ""
  }
  // TODO
  //"icons": { "16": "", "128": "" }
}
